    "express-validator": "^7.0.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^8.0.3",
    "nodemailer": "^6.9.8"
